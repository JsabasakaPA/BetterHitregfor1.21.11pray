  "authors": ["Jass"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/betterhitreg/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "you.jass.betterhitreg.BetterHitreg"
    ]
  },
  "mixins": [
    "betterhitreg.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
